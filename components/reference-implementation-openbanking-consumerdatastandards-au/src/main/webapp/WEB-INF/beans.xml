<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxrs="http://cxf.apache.org/jaxrs"
       xsi:schemaLocation="
         http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
         http://cxf.apache.org/jaxrs http://cxf.apache.org/schemas/jaxrs.xsd">
    <jaxrs:server id="consent" address="/">
        <jaxrs:serviceBeans>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.PersistAuthorizedConsentApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.PreProcessConsentCreationApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.EnrichConsentCreationResponseApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.PreProcessConsentRetrievalApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.PreProcessConsentRevokeApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.PopulateConsentAuthorizeScreenApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.ValidateConsentAccessApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.IssueRefreshTokenApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.ValidateAuthorizationRequestApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.PreProcessClientCreationApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.PreProcessClientUpdateApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.ValidateEventCreationApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.ValidateEventPollingApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.EnrichEventPollingResponseApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.ValidateEventSubscriptionApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.EnrichEventSubscriptionResponseApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.MapAcceleratorErrorResponseApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.EnrichConsentFileResponseApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.PreProcessConsentFileUploadApi"/>
            <bean class="org.wso2.openbanking.consumerdatastandards.au.extensions.gen.api.ValidateConsentFileRetrievalApi"/>
        </jaxrs:serviceBeans>
        <jaxrs:providers>
            <bean class="com.fasterxml.jackson.jaxrs.json.JacksonJsonProvider"/>
        </jaxrs:providers>
    </jaxrs:server>
</beans>
